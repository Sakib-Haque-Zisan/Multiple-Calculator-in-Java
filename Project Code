    private void jbtnEqlActionPerformed(java.awt.event.ActionEvent evt) {                                        
         s_num = Double.parseDouble(display.getText());
        switch(operations)
        {
            case "+";
                 result = f_num + s_num;
                 answer = String.format("%.0f",result);
                 display.setText(answer);
            
            break;
            
            case "-";
            result = f_num - s_num;
                answer = String.format("%.0f",result);
                 display.setText(answer);
            
            
            break;
            
            case "*";
            result = f_num * s_num;
                answer = String.format("%.0f",result);
                 display.setText(answer);
            
            
            break;
            
            case "/";
            result = f_num / s_num;
               answer = String.format("%.0f",result);
                 display.setText(answer);
            
            
            break;
        }

// TODO add your handling code here:
    }                                       

