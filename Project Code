    private void jbtnCoshActionPerformed(java.awt.event.ActionEvent evt) {                                         
       double ops = Double.parseDouble(String.valueOf(display.getText()));
        ops = Math.cosh(ops);
        display.setText(String.valueOf(ops));
    }                                        

